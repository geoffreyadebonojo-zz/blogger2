<h1>All Articles</h1>

<ul id="articles">
  <% @articles.each do |article| %>
    <li>
      <span class="title">
        <%= link_to article.title, article_path(article),
    class: 'article-title', id: "article-#{article.id}" %>

     </span>
      <div class= "timestamp">
        <%= article.created_at %>
      </div>
    </li>
  <% end %>
</ul>
