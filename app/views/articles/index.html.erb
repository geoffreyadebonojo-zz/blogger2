<h1>All Articles</h1>

<ul id="articles">
  <% @articles.each do |article| %>
    <li>
      <p>
        <%= link_to article.title, article_path(article),
    class: 'article-title', id: "article-#{article.id}" %>
      </p>
      <p class= "timestamp">
        <%= article.created_at %>
      </p>
    </li>
  <% end %>
</ul>
<%= link_to "Create a New Article", new_article_path, class: "new-article-link"%>
